.logobg {
  width: 100%;
  height: 300rpx;
  background: linear-gradient(180deg, #DEAB00, #FFDA5D);
}

.uselogos {
  position: relative;
  height: 300rpx;
}

.usetitle {
  position: absolute;
  bottom: 110rpx;
  left: 40rpx;
}

.uselogo {
  width: 115rpx;
  height: 115rpx;
  border-radius: 50%;
  /* border-radius: 50%;
    border: 5rpx solid #cc2a29; */
  float: left;
}

.userinfo {
  margin-left: 24rpx;
  float: left;
  color: #fff;
  font-size: 24rpx;
}

.userinfo .userinfo-b {
  margin-top: 13rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

.updategrade {
  color: red;
  margin-left: 10rpx;
}

.userinfo .username {
  font-size: 32rpx;
}

.userinfo .userinfo-b .user-grade {
  /* background: #d93527; */
  border-radius: 25rpx;
  /* border: 1px solid #cc3125; */
  text-align: center;
  height: 40rpx;
  line-height: 37rpx;
  padding: 0rpx 15rpx;
  float: left;
}

.userinfo .userinfo-b .user-score {
  display: flex;
  float: left;
  border: 1px solid #ffecb3;
  border-radius: 25rpx;
  background: linear-gradient(to bottom, #ffecb3, #ffd550);
  color: #f50;
  height: 40rpx;
  line-height: 37rpx;
  padding: 0rpx 16rpx 0 4rpx;
  margin-left: 24rpx;
}

.userinfo .userinfo-b .user-score text {
  line-height: 40rpx;
  font-size: 24rpx;
}

.user-score image {
  width: 32rpx;
  height: 32rpx;
  margin-top: 4rpx;
  margin-right: 10rpx;
}

.user-score text text {
  font-weight: bold;
}

.usecd {
  display: flex;
  background: #fff;
  padding: 24rpx;
  overflow: hidden;
}

.usecdlist {
  width: 185rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.usecdlist text {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #424242;
}

.usecdlist .order_num {
  border-radius: 16rpx;
  border: 3rpx solid #ff5252;
  background: #fff;
  top: -7rpx;
  position: absolute;
  left: 57%;
  font-size: 20rpx;
  line-height: 32rpx;
  padding: 0 8rpx;
  min-width: 16rpx;
  text-align: center;
  height: 32rpx;
}

.usecdlist .order_num text {
  color: #ff5252;
  line-height: 32rpx;
}

.user_custom {
  width: 714rpx;
  height: 280rpx;
  margin: 0rpx auto;
  transform: translateY(-90rpx);
  background: #FFFFFF;
  box-shadow: 0px 7px 9px 1px rgba(188, 188, 188, 0.2900);
  border-radius: 15rpx;
}

.user_custom .custom_title {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
}

.user_custom .custom_title .title_name {
  width: 100%;
  height: 85rpx;
  padding-top: 10rpx;
  font-size: 34rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.user_custom .custom_title .title_name .name_active {
  width: 62rpx;
  height: 8rpx;
  background: linear-gradient(98deg, rgba(197, 153, 4, 0.4800) 52%, rgba(255, 255, 255, 0) 100%);
}

.user_custom .custom_body {
  display: flex;
  /* flex-direction:column; */
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  height: 180rpx;
  font-size: 30rpx;
}

.user_custom .custom_body .body_waiting {
  width: 33%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.user_custom .custom_body .body_waiting image {
  width: 43rpx;
  height: 49rpx;
}

.user_order {
  width: 716rpx;
  height: 200rpx;
  margin: 0 auto;
  transform: translateY(-60rpx);
  background: #FFFFFF;
  box-shadow: 0px 3px 16px 0px rgba(184, 184, 184, 0.5200);
  display: flex;
  justify-content: center;
  align-items: center;
}

.user_order .order_item {
  width: 33.33%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 28rpx;
}

.user_order .order_item image {
  width: 80rpx;
  height: 80rpx;
}

.hdonglist {
  padding: 32rpx 24rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  position: relative;
  background: #fff;
  margin-top: 24rpx;
  border-bottom: 1rpx solid #e0e0e0;
}

.hdonglist::after,
.hdonglist::before {
  border: 6px solid transparent;
  border-left: 6px solid #fff;
  width: 0;
  position: absolute;
  right: 10px;
  height: 0;
  top: 38rpx;
  content: ' ';
}

.hdonglist::before {
  border-left-color: #bbb;
  right: 8px;
}

page {
  background: #fff;
