  line-height: 40rpx;
}

.hdonglist.outlogin {
  text-align: center;
  color: #f44336;
  border-top: 1rpx solid #e0e0e0;
  border-bottom: 1rpx solid #e0e0e0;
}

.hdonglist.outlogin::after,
.hdonglist.outlogin::before {
  display: none;
}

