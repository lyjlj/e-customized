page {
    background: #f5f5f5;
}

.page-section {
    margin-top: 24rpx;
    overflow: hidden;
    position: relative;
}

.line {
    position: relative;
}

.line view {
    position: absolute;
    border-bottom: 1rpx solid #f0f0f0;
    top: -1rpx;
    left: 0rpx;
    right: 0rpx;
}

.form-group {
    padding: 24rpx;
    font-size: 30rpx;
    line-height: 48rpx;
    overflow: hidden;
    display: flex;
    position: relative;
    color: #212121;
}

.form-group:not(:last-child):after {
    content: '';
    position: absolute;
    bottom: 0;
    right: 0;
    width: 726rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.form-group-name {
    margin-right: 16rpx;
    width: 160rpx;
    overflow: hidden;
}

.form-group-input input {
    width: 510rpx;
}

.form-group .selecttxt {
    color: #bdbdbd;
    text-align: right;
    width: 490rpx;
}

.deposit {
    text-align: right;
    width: 490rpx;
}

.come-icon {
    font-size: 30rpx;
    right: 24rpx;
    position: absolute;
    width: 24rpx;
    height: 48rpx;
    line-height: 48rpx;
    color: #dbdbdb;
    bottom: 20rpx;
}

.btn {
    width: 702rpx;
    margin: 0 auto;
    overflow: hidden;
    text-align: center;
}

.submit {
    color: #fff;
    width: 100%;
    border-radius: 8rpx;
    margin-top: 48rpx;
    font-size: 32rpx;
    line-height: 48rpx;
    padding: 24rpx 0;
    background: linear-gradient(to right,#ff5622 0%,#ff1a1a 100%);
    border: none;
    font-weight: bold;
}

.coupontitle {
    line-height: 48rpx;
    display: flex;
    font-size: 32rpx;
    position: fixed;
    top: 0;
    z-index: 99;
    width: 702rpx;
    padding: 24rpx;
    padding-bottom: 0;
}

.coupontitlel {
    width: 50%;
    text-align: center;
}

.coupontitlel view {
    border-bottom: 4rpx solid #fff;
    width: 160rpx;
    margin: 0 auto;
    font-size: 32rpx;
    color: #212121;
    font-weight: 500;
    padding-bottom: 24rpx;
}

.coupontitlel view.active {
    border-bottom: 4rpx solid #212121;
    font-weight: bold;
    font-size: 32rpx;
}

.mt120 {
    margin-top: 100rpx;
    position: relative;
}

.couponlist {
    margin-top: 24rpx;
}

.coupon-info {
    width: 702rpx;
    margin: 0 auto;
    display: block;
    overflow: hidden;
    border-radius: 0 0 8px 8rpx;
}

.coupon-pic {
    position: relative;
    overflow: hidden;
    color: #fff;
    line-height: 40rpx;
    padding-bottom: 18rpx;
}

.coupon-pic .counpimg {
    height: 86rpx;
    padding-bottom: 0;
}

.coupon-money {
    font-size: 48rpx;
    font-family: SFUIDisplay-Medium;
    font-weight: 700;
    line-height: 64rpx;
    float: left;
}

.coupon-type {
    float: right;
    font-size: 24rpx;
    line-height: 40rpx;
}

.lqicon3 {
    width: 702rpx;
    height: 96rpx;
}

.coupon-des {
    padding: 0 24rpx 16rpx 24rpx;
    overflow: hidden;
    font-size: 24rpx;
}

.coupon-des .coupon-txt {
    font-size: 24rpx;
    line-height: 32rpx;
    color: #424242;
    padding-bottom: 8rpx;
}

.coupon .coupon-txt:before {
    content: "";
    width: 10rpx;
    height: 10rpx;
    background: #ff5252;
    display: inline-block;
    margin-right: 15rpx;
    margin-bottom: 5rpx;
}

.coupon-over .coupon-txt:before {
    content: "";
    width: 10rpx;
    height: 10rpx;
    background: #d4d4d4;
    display: inline-block;
    margin-right: 15rpx;
    margin-bottom: 5rpx;
}

.coupon-use .coupon-txt:before {
    content: "";
    width: 10rpx;
    height: 10rpx;
    background: #ffcdd2;
    display: inline-block;
    margin-right: 15rpx;
    margin-bottom: 5rpx;
}

.kong {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 144rpx 0 285rpx 0;
    min-height: 680rpx;
    overflow: hidden;
    font-size: 28rpx;
    color: #212121;
}

.bg_nullCounp {
    width: 192rpx;
    height: 192rpx;
    padding: 32rpx;
}

.Draw_total {
    padding: 24rpx;
    font-size: 32rpx;
    line-height: 48rpx;
    overflow: hidden;
    display: flex;
    position: relative;
    background: #fff;
    color: #212121;
}

.notice {
    padding: 0 24rpx;
    color: #747474;
    font-size: 26rpx;
    line-height: 40rpx;
    margin: 16rpx 0 24rpx 0;
}

.forget {
    text-align: right;
    font-size: 26rpx;
    color: #747474;
    margin: 8rpx 24rpx 0rpx 0;
}

.time {
    padding: 8rpx 24rpx 0 24rpx;
    font-size: 26rpx;
    color: #747474;
    line-height: 40rpx;
}

.points-hd {
    background: #ff5722;
    padding: 24rpx;
    padding-bottom: 16rpx;
    color: #fff;
    margin-bottom: 24rpx;
    display: flex;
}

.points-hd .totalPoint {
    font-size: 64rpx;
    margin-right: 16rpx;
}

.txt {
    font-size: 36rpx;
    line-height: 96rpx;
}

.points-list {
    background: #fff;
}

.point-li {
    border-bottom: 1rpx #f0f0f0 solid;
    overflow: hidden;
}

.point-li:last-child {
    border-bottom: none;
}

.point-left {
    float: left;
    width: 540rpx;
    color: #212121;
    font-size: 32rpx;
}

.point-left view {
    float: left;
    line-height: 48rpx;
}

.Drawtime {
    font-size: 24rpx;
    color: #bdbdbd;
    width: 100%;
}

.point-count {
    font-size: 32rpx;
    color: #212121;
    line-height: 96rpx;
    float: left;
    width: 160rpx;
    text-align: right;
}