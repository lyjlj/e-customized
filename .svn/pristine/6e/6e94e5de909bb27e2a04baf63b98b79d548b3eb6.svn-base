.fx_header {
    padding-top: 24rpx;
}

.fx_Info {
    overflow: hidden;
    border-bottom: 1px #f0f0f0 solid;
}

.fxInfoBox {
    border-radius: 24rpx;
    background: #fff;
    margin: 24rpx;
    margin-bottom: 0;
    padding: 24rpx 0 0 24rpx;
    overflow: hidden;
    box-shadow: 0px 5px 20px rgba(156,39,176,.4);
}

.fx_userInfo {
    float: left;
}

.fx_hd {
    overflow: hidden;
    margin-bottom: 8rpx;
    line-height: 48rpx;
}

.fx_name {
    font-size: 32rpx;
    font-weight: bold;
    color: #212121;
    margin-right: 16rpx;
    float: left;
    line-height: 48rpx;
}

.btn {
    background: #fff8e1;
    line-height: 32rpx;
    color: #ffb300;
    font-size: 20rpx;
    padding: 0rpx 16rpx;
    float: left;
    border: 1px #ffc107 solid;
    border-radius: 16rpx;
    margin-top: 4rpx;
}

.name_desc {
    color: #747474;
    font-size: 24rpx;
    line-height: 40rpx;
    margin-bottom: 24rpx;
    font-weight: 500;
}

.name_desc text {
    color: #212121;
    margin: 0 8rpx;
    font-weight: bold;
}

.drawalsBox {
    overflow: hidden;
    padding: 24rpx;
    padding-left: 0;
}

.drawals {
    width: 33.3%;
    float: left;
}

.drawals .drawals_width {
    font-size: 32rpx;
    font-weight: bold;
    color: #212121;
    display: block;
    line-height: 48rpx;
    text-align: center;
}

.drawals .drawals_width .yuan {
    font-size: 24rpx;
    margin-right: 8rpx;
    display: inline-block;
}

.drawals .drawals_width text {
    font-size: 32rpx;
    line-height: 40rpx;
    color: #212121;
    text-align: center;
    display: inline-block;
}

.drawals text {
    font-size: 22rpx;
    line-height: 40rpx;
    color: #bdbdbd;
    text-align: center;
    display: block;
}

.fx_common {
    padding: 0 24rpx;
    background: #fff;
    overflow: hidden;
    margin-top: 32rpx;
}

.fx_common .fx_li {
    display: block;
    border-bottom: 1px #f7f7f7 solid;
    padding: 24rpx 0;
    font-size: 30rpx;
    color: #212121;
    line-height: 48rpx;
}

.fx_common .fx_li text {
    font-size: 30rpx;
    color: #212121;
}

.fx_common .fx_li .fx_icon {
    margin-right: 16rpx;
    width: 48rpx;
    height: 48rpx;
    float: left;
}

.Distribution {
    float: right;
    color: #fff;
    font-size: 24rpx;
    line-height: 36rpx;
    border-radius: 28rpx 0 0 28rpx;
    padding: 8rpx 28rpx;
    background: #f57c00;
    background: -moz-linear-gradient(top,#f57c00,#ff9800);
    background: -webkit-gradient(left,0 0,0 bottom,from(#f57c00),to(#ff9800));
    background: -o-linear-gradient(right,#f57c00,#ff9800);
    background: linear-gradient(to right,#f57c00,#ff9800);
}

.right_size {
    width: 24rpx;
    height: 48rpx;
    color: #bdbdbd;
    margin-right: 8rpx;
    font-size: 30rpx;
}
.service{
    background:#fff;
  width:750rpx;
  display:inline-block;
  padding-bottom: 50rpx;
  }
  .service .serviceitem{
  width:250rpx;
  height:150rpx;
  text-align:center;
  display:inline-block;
  position:relative;
  }
  
  .serviceitem image{
      width:60rpx;
  margin:auto;
  height:60rpx;
  margin-top:30rpx;
  }
  .serviceitem text{
    font-size: 28rpx;
    color:#666666;
    line-height: 30px
  }
  .titleTag {
    padding: 32rpx 24rpx;
    line-height: 40rpx;
    font-size: 32rpx;
    position: relative;
    background: #fff;
    margin-top: 24rpx;
    border-bottom: 1rpx solid #e0e0e0;
}