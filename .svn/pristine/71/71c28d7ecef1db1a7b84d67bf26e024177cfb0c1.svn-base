.qianggtitle {
    text-align: center;
    margin-top: 35rpx;
    line-height: 50rpx;
    padding: 0 50rpx;
    position: relative;
}

.qianggtitle text {
    padding: 30rpx;
    font-size: 18px;
}

.qianggtitle::after {
    content: " ";
    width: 40rpx;
    border-top: 2px solid #455052;
    border-bottom: 3px solid #455052;
    height: 3rpx;
    line-height: 20rpx;
    padding-top: 3rpx;
    position: absolute;
    top: 20rpx;
    left: 230rpx;
}

.qianggtitle::before {
    content: " ";
    width: 40rpx;
    border-top: 2px solid #455052;
    border-bottom: 3px solid #455052;
    height: 3rpx;
    line-height: 20rpx;
    padding-top: 3rpx;
    position: absolute;
    top: 20rpx;
    left: 480rpx;
}

.qglist {
    width: 685rpx;
