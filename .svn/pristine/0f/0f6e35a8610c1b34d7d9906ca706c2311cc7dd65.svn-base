/* 伪元素设置倒三角 */
.jiages::after {
  border: 8rpx solid transparent;
  border-top: 8rpx solid #212121;
  width: 0;
  height: 0;
  position: absolute;
  right: 10rpx;
  top: 31rpx;
  content: ' ';
  color: #212121;
}

.jiages::before {
  border: 8rpx solid transparent;
  border-bottom: 8rpx solid #212121;
  width: 0;
  height: 0;
  position: absolute;
  content: ' ';
  right: 10rpx;
  top: 10rpx;
  color: #212121;
}

.jiangxu::after {
  border: 8rpx solid transparent;
  border-top: 8rpx solid #bf4edc;
