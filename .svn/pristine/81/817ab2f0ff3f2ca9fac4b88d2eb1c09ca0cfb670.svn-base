}

.cursor {
  border-right: 2px solid #000;
  padding-right: 3px;
  animation: cursort 1s infinite steps(1, start);
}

@keyframes cursort {
  0% {
    border-right: 2px solid white;
  }

  50% {
    border-right: 2px solid #000;
  }

  100% {
    border-right: 2px solid white;
  }
}

