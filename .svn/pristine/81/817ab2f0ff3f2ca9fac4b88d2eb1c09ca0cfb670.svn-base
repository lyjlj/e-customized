.favorite-animation-container {
  right: 1px;
  height: 140rpx;
  width: 150rpx;
  margin-left: 0.133333rem;
  position: absolute;
  bottom: 100rpx;
}

.favorite-animation-container image {
  position: absolute;
  bottom: 0;
  left: 45%;
  border-radius: 50%;
}

.favorite-animation {
  -webkit-animation-name: appear-from-bottom;
  animation-name: appear-from-bottom;
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
  -webkit-animation-timing-function: cubic-bezier(0.47, 0, 0.745, 0.715);
  animation-timing-function: cubic-bezier(0.47, 0, 0.745, 0.715);
}

@keyframes appear-from-bottom {
  0% {
    opacity: 0.5;
    width: 40rpx;
    height: 40rpx;
    display: block;
  }

  10% {
    opacity: 1;
  }

  20% {
    width: 75.5rpx;
    height: 75.5rpx;
  }

  66% {
    opacity: 1;
  }

  to {
    -webkit-transform: translateY(-420rpx);
    transform: translateY(-420rpx);
    opacity: 0;
    width: 40rpx;
    height: 40rpx;
    display: none;
  }
}

.btn {
  display: block;
  padding-left: 0px;
  padding-right: 0px;
  line-height: 10px;
  background-color: rgba(255, 255, 255, 0);
}

.btn::after {
  width: 0;
  height: 0;
  top: 0;
  left: 0;
  
}

.btn_hover {
